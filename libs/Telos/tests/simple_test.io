#!/usr/bin/env io

// Simple test for Telos start
writeln("Testing Telos start...")

// Load the TelosBridge addon directly
addon := Addon clone setRootPath("build/addons") setName("TelosBridge")

writeln("Attempting to load addon...")
result := addon load
if(result isNil,
    "❌ Addon load returned nil" println
    exit(1)
)
"✅ Addon loaded successfully" println

// Check if protos were registered
if(Lobby hasSlot("TelosBridge"),
    TelosBridgePrototype := Lobby TelosBridge
    "✅ TelosBridge proto found after addon load" println
,
    "❌ TelosBridge proto not found after addon load attempt" println
    exit(1)
)

writeln("✅ Test completed successfully - addon loading works")